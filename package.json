{
  "name": "git-editor",
  "version": "1.0.0",
  "description": "A terminal editor designed for git",
  "bin": {
    "git-editor": "dist/cli.js"
  },
	"files": ["dist"],
  "scripts": {
    "start": "NODE_ENV=development node src/cli.js",
    "build": "npm run compile",
    "compile": "babel src -d dist",
    "test": "xo"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "xo/esnext",
      "xo-react"
    ]
  },
  "keywords": [
    "git",
    "editor",
    "cli",
    "commit"
  ],
  "author": "Mario Nebl <mario.nebl@sinnerschrader.com>",
  "license": "MIT",
  "dependencies": {
    "autobind-decorator": "^1.3.3",
    "babel-traverse": "^6.10.4",
    "babel-types": "^6.10.2",
    "blessed": "^0.1.81",
    "common-tags": "^1.3.0",
    "conventional-commits-parser": "^1.2.2",
    "css-layout": "^1.1.1",
    "gitconfig": "^2.0.0",
    "glob": "^7.0.5",
    "ignore": "^3.1.3",
    "lodash": "^4.13.1",
    "meow": "^3.7.0",
    "mz": "^2.4.0",
    "react": "^0.14.8",
    "react-blessed": "^0.1.7",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "@marionebl/babel-live": "^1.3.0",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint-config-xo-react": "^0.8.0",
    "eslint-plugin-react": "^5.2.2",
    "rc": "^1.1.6",
    "xo": "^0.16.0"
  }
}
